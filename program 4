/*Program for binary search algorithm*/
#include<stdio.h>
int main()
{
int I,a[100],n, low, high, mid, b;
printf(“Enter number of elements:”);
scanf(“%d”, &n);
printf(“Enter %d integers:”, n);

for (i= 0; i&lt; n; i++)
scanf(“%d”, &a[i]);
printf(“Enter value to search:”);
scanf(“%d”,&;b);

low = 0;
high = n – 1;
mid = (low+high)/2;

while (low<= high) {
if (a[mid]< b)
low = mid + 1;
else if (a[mid] == b) {
printf(“%d found at position %d”, b, mid+1);
break;
}
else
high = mid – 1;

mid = (low + high)/2;

}
if (low>high)
printf(“The number %d not found in the list”, search);

return 0;
}
